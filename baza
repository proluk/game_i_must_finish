  KEY `FK_authorizedconnections_account` (`id`),
  CONSTRAINT `FK_authorizedconnections_account` FOREIGN KEY (`id`) REFERENCES `account` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=14 DEFAULT CHARSET=utf8;

